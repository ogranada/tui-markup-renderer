  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'tui-markup'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=tui-markup"
        ],
        "filter": {
          "name": "tui-markup",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug integration test 'markup_parser_tests'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--test=markup_parser_tests",
          "--package=tui-markup"
        ],
        "filter": {
          "name": "markup_parser_tests",
          "kind": "test"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}